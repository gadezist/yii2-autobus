{
    "name": "hiqdev/yii2-autobus",
    "type": "yii2-extension",
    "description": "Simple automated command bus",
    "keywords": [
        "yii2",
        "autobus"
    ],
    "homepage": "https://github.com/hiqdev/yii2-autobus",
    "license": "BSD-3-Clause",
    "support": {
        "email": "support@hiqdev.com",
        "source": "https://github.com/hiqdev/yii2-autobus",
        "issues": "https://github.com/hiqdev/yii2-autobus/issues",
        "wiki": "https://github.com/hiqdev/yii2-autobus/wiki",
        "forum": "http://forum.hiqdev.com/"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "role": "Project lead",
            "email": "sol@hiqdev.com",
            "homepage": "http://hipanel.com/"
        },
        {
            "name": "Dmitry Naumenko",
            "role": "Lead backend developer",
            "email": "d.naumenko.a@gmail.com",
            "homepage": "http://silverfire.me/"
        },
        {
            "name": "Andrey Klochok",
            "role": "Lead frontend developer",
            "email": "andreyklochok@gmail.com",
            "homepage": "http://hiqdev.com/"
        },
        {
            "name": "Yuriy Myronchuk",
            "role": "QA Lead",
            "email": "bladeroot@gmail.com",
            "homepage": "http://hiqdev.com/"
        }
    ],
    "require": {
        "league/tactician": "^1.0"
    },
    "require-dev": {
        "yiisoft/yii2": "dev-master",
        "phpunit/phpunit": "^8.0",
        "hiqdev/hidev-php": "dev-master",
        "hiqdev/hidev-hiqdev": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\yii2\\autobus\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "1.0": "1.0.x-dev"
        },
        "config-plugin": {
            "tests": ["$common", "config/tests.php"],
            "params": "config/params.php",
            "common": "config/common.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
